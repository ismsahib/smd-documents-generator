{
  "name": "react-template",
  "version": "0.1.0",
  "description": "react webpack template",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "reduxjs/toolkit",
    "react-router",
    "eslint",
    "prettier",
    "husky"
  ],
  "author": "ismsahib",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ismsahib/webpack-react-typescript-template"
  },
  "scripts": {
    "start": "yarn webpack serve --progress --mode development",
    "clean": "rm -rf ./build",
    "build": "yarn clean && webpack --progress --mode production",
    "lint": "yarn eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --no-error-on-unmatched-pattern",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config .prettierrc.js --check src/**/*.{js,jsx,ts,tsx}",
    "type": "./node_modules/.bin/tsc --project tsconfig.json --noEmit",
    "lint-staged": "lint-staged",
    "husky-install": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@svgr/webpack": "^8.0.1",
    "@types/classnames": "^2.3.1",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "babel-loader": "^9.1.2",
    "classnames": "^2.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.43.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "typescript-plugin-css-modules": "^5.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "buffer": "^6.0.3",
    "docx": "^8.0.4",
    "docxtemplater": "^3.37.11",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "pizzip": "^3.1.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --max-warnings=0 --fix",
      "git add"
    ],
    "*.{graphql,json,scss,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  }
}
